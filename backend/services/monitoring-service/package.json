  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.17",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.19",
    "@types/pg": "^8.15.5",
