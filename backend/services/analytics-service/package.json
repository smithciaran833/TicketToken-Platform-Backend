{
  "name": "@tickettoken/analytics-service",
  "version": "1.0.0",
  "description": "Analytics service for TicketToken platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "db:migrate": "ts-node scripts/db/run-migrations.ts",
    "db:health": "ts-node scripts/db/health-check.ts",
    "db:setup": "npm run db:migrate && npm run db:health",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "@tickettoken/shared": "file:../../shared",
    "amqplib": "^0.10.9",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.6.1",
    "events": "^3.3.0",
    "exceljs": "^4.3.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.20.0",
    "mongoose": "^7.4.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "redis": "^5.8.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/bull": "^4.10.4",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.196",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.4.5",
    "@types/pdfkit": "^0.17.2",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "axios": "^1.11.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=20 <21"
  },
  "types": "dist/index.d.ts"
}
