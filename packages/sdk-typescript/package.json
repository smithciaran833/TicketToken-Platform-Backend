{
  "name": "@tickettoken/sdk-typescript",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for TicketToken platform - NFT ticketing and event management",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "SECURITY.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "keywords": [
    "tickettoken",
    "sdk",
    "typescript",
    "javascript",
    "tickets",
    "events",
    "nft",
    "blockchain",
    "solana",
    "ticketing",
    "event-management",
    "web3"
  ],
  "author": "TicketToken Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tickettoken/sdk-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/tickettoken/sdk-typescript/issues"
  },
  "homepage": "https://docs.tickettoken.com",
  "dependencies": {
    "axios": "^1.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
